<table class="table">
    <tr>
        <th scope="col">Project ID</th>
        <th scope="col">Project Name</th>
        <th scope="col">Status</th>
        <th scope="col">Actions</th>
    </tr>
    <% @projects.each do |p|%>
        <tr>
            <td><%=p.id%></td>
            <td><%=p.name%></td>
            <td><%=p.status%></td>
            <td><%=link_to "View Live", url: p.url%></td>
            <td><%=link_to "Edit", edit_member_project_path(p)%></td>
        </tr>
    <%end%>
</table>
